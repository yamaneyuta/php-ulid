<?xml version="1.0" encoding="UTF-8"?>
<ruleset>
    <file>./src</file>
    <file>./tests</file>
    <rule ref="PSR12">
        <!-- Visibility must be declared on all constants if your project supports PHP 7.1 or later -->
        <!-- PHP7.1未満も対象のため除外 -->
        <exclude name="PSR12.Properties.ConstantVisibility" />
    </rule>
</ruleset>